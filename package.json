{
  "name": "jmd-scripts",
  "version": "0.0.0-semantically-released",
  "description": "CLI for common scripts for my projects",
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "bin": {
    "jmd-scripts": "dist/index.js"
  },
  "scripts": {
    "add-contributor": "node src contributors add",
    "test": "node src test",
    "test:update": "node src test --updateSnapshot",
    "build": "node src build",
    "lint": "node src lint",
    "format": "node src format",
    "validate": "node src validate"
  },
  "files": [
    "dist",
    "babel.js",
    "eslint.js",
    "config.js",
    "prettier.js",
    "jest.js"
  ],
  "keywords": [],
  "author": "Jeff Detmer <jeff.detmer@gmail.com> (http://jeffdetmer.com/)",
  "license": "MIT",
  "dependencies": {
    "all-contributors-cli": "4.11.1",
    "arrify": "1.0.1",
    "babel-cli": "6.26.0",
    "babel-jest": "23.0.0-alpha.0",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-dynamic-import-webpack": "1.0.2",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-macros": "2.2.1",
    "babel-plugin-minify-dead-code-elimination": "0.4.3",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "concurrently": "3.5.1",
    "cross-env": "5.1.5",
    "cross-spawn": "6.0.5",
    "doctoc": "1.3.1",
    "eslint": "4.19.1",
    "eslint-config-kentcdodds": "13.0.1",
    "eslint-config-prettier": "2.9.0",
    "glob": "7.1.2",
    "husky": "0.15.0-rc.7",
    "is-ci": "1.1.0",
    "jest": "22.4.2",
    "lint-staged": "7.1.0",
    "lodash.camelcase": "4.3.0",
    "lodash.has": "4.5.2",
    "mkdirp": "0.5.1",
    "prettier": "1.12.1",
    "read-pkg-up": "3.0.0",
    "resolve": "1.7.1",
    "rimraf": "2.6.2",
    "rollup": "0.59.1",
    "rollup-plugin-babel": "3.0.4",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-json": "3.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.2.1",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-uglify": "3.0.0",
    "which": "1.3.0",
    "yargs-parser": "10.0.0"
  },
  "devDependencies": {
    "jest-in-case": "1.0.2",
    "slash": "2.0.0"
  },
  "eslintConfig": {
    "extends": ["kentcdodds", "kentcdodds/jest"],
    "rules": {
      "no-process-exit": "off",
      "import/no-dynamic-require": "off",
      "import/no-unassigned-import": "off",
      "no-console": "off",
      "no-nested-ternary": "off"
    }
  },
  "eslintIgnore": ["node_modules", "coverage", "dist"],
  "husky": {
    "hooks": {
      "pre-commit": "node src precommit"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shellthor/jmd-scripts.git"
  },
  "bugs": {
    "url": "https://github.com/shellthor/jmd-scripts/issues"
  },
  "homepage": "https://github.com/shellthor/jmd-scripts#readme"
}
